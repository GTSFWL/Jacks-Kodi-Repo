<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="skin.carpc-xtouch" version="2.4.0" name="CarPC-XTouch" provider-name="JackTirol">
  <requires>
    <import addon="xbmc.gui" version="5.9.0"/>
  </requires>
  <extension point="xbmc.gui.skin" defaultthemename="Textures.xbt" effectslowdown="0.76" debugging="false">
    <res width="1280" height="720" aspect="16:9" folder="16x9" />
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">CarPC xtouch screen skin</summary>
    <description lang="en">The skin is based on re-Touched skin(by Jezz_X), modified by Idorel and reworked by JackTirol. See changelog for a list of all modifications. 
    - </description>
    <disclaimer lang="en">Use this skin with a touch screen or a keyboard (the pointer is not being shown)</disclaimer>
    <platform>all</platform>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum></forum>
    <website>http://raspicarprojekt.de/showthread.php?tid=790</website>
    <email></email>
    <source></source>
  </extension>
</addon>
<addon id="plugin.program.carpc-xtouch" name="X-Touch Helper" version="0.1.1" provider-name="Marc Hillesheim">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="start"/>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary lang="en">X-Touch Helper plugin</summary>
        <description lang="en">X-Touch Helper plugin for Jacks X-Touch Skin</description>
        <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
        <forum></forum>
        <email></email>
    </extension>
</addon>
</addons>
